{
    "name": "dario_swain/ds-recaptcha-bundle",
    "type": "symfony-bundle",
    "description": "Google ReCAPTCHA (ver. 2) Symfony Bundle.",
    "keywords": ["recaptcha", "bundle", "symfony", "form", "validator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Pokamestov",
            "homepage": "http://github.com/DarioSwain"
        }
    ],
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "google/ReCAPTCHA",
                "version":"dev-master",
                "source": {
                    "url": "https://github.com/google/ReCAPTCHA.git",
                    "type": "git",
                    "reference":"master"
                },
                "autoload": {
                    "files": ["php/recaptchalib.php"]
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "dario_swain/ds-recaptcha": "1.0.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "DS\\ReCaptchaBundle\\": "" }
    }
}